{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "symfony/console": "^4.3",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.3",
        "symfony/yaml": "^4.3",
        "urbanetter/jassy": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "symfony/dotenv": "^4.3",
        "symfony/maker-bundle": "^1.11",
        "symfony/web-server-bundle": "^4.3",
        "theofidry/psysh-bundle": "^3.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/urbanetter/jassy"
        }
    ],
    "extra": {
        "symfony": {
            "id": "01BTDNTQABV8SZ6ZMDJQXCBBHN",
            "allow-contrib": false
        }
    }
}
